{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zellow777-covid19-adf"
		},
		"AzureBlobStorageCovid19_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageCovid19'"
		},
		"AzureDataLakeStorageCovid19_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageCovid19'"
		},
		"AzureDataLakeStorageCovid19_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zellow777covid19dl.dfs.core.windows.net/"
		},
		"event trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2e981504-a846-44b6-9111-5536e39db261/resourceGroups/covid-19-rg/providers/Microsoft.Storage/storageAccounts/covidsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zellow777-covid19-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageCovid19_connectionString": {
						"value": "[parameters('AzureBlobStorageCovid19_connectionString')]"
					},
					"AzureDataLakeStorageCovid19_accountKey": {
						"value": "[parameters('AzureDataLakeStorageCovid19_accountKey')]"
					},
					"AzureDataLakeStorageCovid19_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageCovid19_properties_typeProperties_url')]"
					},
					"event trigger_properties_typeProperties_scope": {
						"value": "[parameters('event trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}